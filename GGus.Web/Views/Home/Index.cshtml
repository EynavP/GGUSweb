<section classe="abc" style="padding:0px 0px 60px 0px;">
    <a href="\Home\CommingSoon">
        <div id="commingsoon" class="carousel slide" data-ride="carousel">
            <div class="tag-gallery"><h5>Coming Soon !!!</h5></div>
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="~/images/pictures/BATLEGROUNDS.jpg" class="d-block w-100" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/pictures/OUTRIDES.jpg" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/pictures/img_5terre_wide.jpg" alt="Third slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/pictures/img_mountains_wide.jpg" alt="four slide">
                </div>
            </div>
            <p class="carousel-control-prev" href="#commingsoon" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only" s>Previous</span>
            </p>
            <p class="carousel-control-next" href="#commingsoon" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </p>
        </div>
    </a>
</section>

